local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "测试版本1.0", HidePremium = false, SaveConfig = true,IntroText = "通用脚本", ConfigFolder = "OrionTest"})
OrionLib:MakeNotification({
	Name = "欢迎",
	Content = "欢迎来到bilisolongcheng脚本制作",
	Image = "rbxassetid://4483345998",
	Time = 10
})
local rw = Window:MakeTab({
	Name = "主要功能",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Doors = Window:MakeTab({
	Name = "Doors",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local bgc = Window:MakeTab({
	Name = "兵工厂",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Doors:AddSection({
	Name = "完整脚本"
})
local Section = rw:AddSection({
	Name = "人物功能，所有游戏通用"
})
local Section = bgc:AddSection({
	Name = "测试版本，脚本可能不完整"
})
rw:AddTextbox({
	Name = "人物速度",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end	  
})
rw:AddButton({
	Name = "飞行",
	Callback = function()
      		loadstring(game:HttpGet(("https://raw.githubusercontent.com/bilisolongcheng/roblox-/main/fx"),true))()
  	end    
})
Doors:AddButton({
	Name = "Doors脚本1号(msdoors)",
	Callback = function()
      		loadstring(game:HttpGet(("https://raw.githubusercontent.com/mstudio45/poopdoors_edited/main/poopdoors_edited.lua"),true))()
  	end    
})
Doors:AddButton({
	Name = "Doors脚本2号(电脑版可用)",
	Callback = function()
      		loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Doors/Script.lua"))()
  	end    
})
Doors:AddButton({
	Name = "Doors－a1000自动走(手机可用)",
	Callback = function()
      		loadstring(game:HttpGet(("https://raw.githubusercontent.com/bilisolongcheng/roblox-/main/a1000"),true))()
        end    
})
