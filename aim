local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
OrionLib:MakeNotification({
	Name = "系统",
	Content = "欢迎测试人员",
	Image = "rbxthumb://type=Asset&id=5107182114&w=150&h=150",
	Time = 2
})
local Window = OrionLib:MakeWindow({Name = "GA", HidePremium = false, SaveConfig = true,IntroText = "通用脚本", ConfigFolder = "OrionTest"})

local statement = Window:MakeTab({
	Name = "警告",
	Icon = "rbxassetid://14359155637",
	PremiumOnly = false
})
statement:AddParagraph("使用教程", "脚本不在指定游戏执行，会发生传送别处的使用的情况，请明细")
statement:AddParagraph("更新时间", "2024.1.19")
local Tab = Window:MakeTab({
	Name = "通用功能",
	Icon = "rbxassetid://14481493145",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "管理员指令 开发者脚本",
	Callback = function()
		loadstring(game:HttpGet(("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"), true))()
	end
})

Tab:AddButton({
	Name = "飞行GUI-V3",
	Callback = function()
		loadstring(game:HttpGet(("https://raw.githubusercontent.com/UWUBeiFeng/Scripts/main/FlyGUIV3.lua"), true))()
	end
})

Tab:AddButton({
	Name = "踏空行走",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Float"))()
	end
})

Tab:AddToggle({
	Name = "夜视",
	Default = false,
	Callback = function(Value)
		if Value then
			game.Lighting.Ambient = Color3.new(1, 1, 1)
		else
			game.Lighting.Ambient = Color3.new(0, 0, 0)
		end
	end
})

OrionLib:Init()
