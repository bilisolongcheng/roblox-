local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "通用脚本", HidePremium = false, SaveConfig = true,IntroText = "脚本中心", ConfigFolder = "OrionTest"})
OrionLib:MakeNotification({
	Name = "欢迎",
	Content = "欢迎来到bilisolongcheng脚本制作",
	Image = "rbxassetid://4483345998",
	Time = 10
})
local rw = Window:MakeTab({
	Name = "主要功能",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Doors = Window:MakeTab({
	Name = "Doors",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local ylxx = Window:MakeTab({
	Name = "一路向西",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = Doors:AddSection({
	Name = "可用脚本"
})
local Section = rw:AddSection({
	Name = "人物功能，所有游戏通用"
})
local Section = ylxx:AddSection({
	Name = "                              !!!一路向西部分脚本需要加群验证可用!!!"
})
rw:AddTextbox({
	Name = "人物速度",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end	  
})
rw:AddTextbox({
	Name = "跳跃高度",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
	end
})
rw:AddTextbox({
	Name = "重力",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		game.Workspace.Gravity = Value
	end
})
rw:AddButton({
	Name = "飞行",
	Callback = function()
      		loadstring(game:HttpGet(("https://raw.githubusercontent.com/bilisolongcheng/roblox-/main/fx"),true))()
  	end    
})
rw:AddToggle({
	Name = "踏空",
	Default = false,
	Callback = function(Value)
		Jump = Value
        game.UserInputService.JumpRequest:Connect(function()
            if Jump then
                game.Players.LocalPlayer.Character.Humanoid:ChangeState("Jumping")
            end
        end)
	end
})
rw:AddToggle({
	Name = "穿墙",
	Default = false,
	Callback = function(Value)
		Noclip = Value
		Stepped = game.RunService.Stepped:Connect(function()
			if Noclip then
				for a, b in pairs(game.Workspace:GetChildren()) do
                    if b.Name == game.Players.LocalPlayer.Name then
                        for i, v in pairs(game.Workspace[game.Players.LocalPlayer.Name]:GetChildren()) do
                            if v:IsA("BasePart") then
                                v.CanCollide = false
                            end
                        end
                    end
                end
			else
				Stepped:Disconnect()
			end
		end)
	end
})
Doors:AddButton({
	Name = "Doors脚本1号(msdoors)",
	Callback = function()
      		loadstring(game:HttpGet(("https://raw.githubusercontent.com/mstudio45/poopdoors_edited/main/poopdoors_edited.lua"),true))()
  	end    
})
