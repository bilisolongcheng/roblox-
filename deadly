local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "致命公司" .. Fluent.Version,
    SubTitle = "by 龙城",
    TabWidth = 160,
    Size = UDim2.fromOffset(320, 220),
    Acrylic = false, 
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl
})
local T = {
    Main = Window:AddTab({ Title = "通用功能", Icon = "rbxassetid://14380684950" }),
}
local Options = Fluent.Options

T.Main:AddButton({
        Title = "管理员指令",
        Description = "开发者指令",
        Callback = function()
            loadstring(game:HttpGet(("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"), true))()
        end
    })
T.Main:AddButton({
        Title = "飞行",
        Description = "人物飞行",
        Callback = function()
           loadstring(game:HttpGet(('https://raw.githubusercontent.com/bilisolongcheng/roblox-/ASX/fx')))()
        end
    })
T.Main:AddButton({
        Title = "无限跳跃",
        Description = "人物无限跳跃 人物跳跃固定高度",
        Callback = function()
            Window:Dialog({
                Title = "无限跳跃",
                Content = "2个版本 无限跳跃＝可以一直跳 踏空跳跃＝固定高度",
                Buttons = {
                    {
                        Title = "无限跳跃",
                        Callback = function()
                            loadstring(game:HttpGet("https://pastebin.com/raw/V5PQy3y0", true))()
                        end
                    },
                    {
                        Title = "踏空跳跃",
                        Callback = function()
                            loadstring(game:HttpGet("https://raw.githubusercontent.com/GhostPlayer352/Test4/main/Float"))()
                        end
                    }
                }
            })
        end
    })
local Y = {
    Main = Window:AddTab({ Title = "控制栏", Icon = "rbxassetid://14380684950" }),
}
Y.Main:AddButton({
        Title = "飞船 大门",
        Description = "控制飞船大门开关",
        Callback = function()
            Window:Dialog({
                Title = "飞船 大门",
                Content = "控制飞船大门开/关",
                Buttons = {
                    {
                        Title = "开",
                        Callback = function()
                            local args = {
    [1] = workspace.Ship.Build.DoorControl.hit,
    [2] = true
}

game:GetService("ReplicatedStorage").Remotes.Door:InvokeServer(unpack(args))
                        end
                    },
                    {
                        Title = "关",
                        Callback = function()
                            local args = {
    [1] = workspace.Ship.Build.DoorControl.hit,
    [2] = false
}

game:GetService("ReplicatedStorage").Remotes.Door:InvokeServer(unpack(args))
                        end
                    }
                }
            })
        end
    })
Y.Main:AddButton({
        Title = "飞船 启动起飞",
        Description = "隔空拉拉杆",
        Callback = function()
            local args = {
    [1] = workspace.Ship.Build.Stol.Lever.hitbox
}

game:GetService("ReplicatedStorage").Remotes.Lever:InvokeServer(unpack(args))
        end
    })
Y.Main:AddButton({
        Title = "飞船 传送按钮",
        Description = "传送器按钮",
        Callback = function()
            game:GetService("ReplicatedStorage").Remotes.Teleport:FireServer()
        end
    })
Y.Main:AddButton({
        Title = "建筑 主门",
        Description = "隔空开主门并进入",
        Callback = function()
            local args = {
    [1] = false
}

game:GetService("ReplicatedStorage").Remotes.Complex:FireServer(unpack(args))
        end
    })
Y.Main:AddButton({
        Title = "建筑 红门",
        Description = "隔空开红门并进入",
        Callback = function()
            local args = {
    [1] = true
}

game:GetService("ReplicatedStorage").Remotes.Complex:FireServer(unpack(args))
        end
    })
Y.Main:AddButton({
        Title = "飞船 灯光",
        Description = "飞船灯光开关",
        Callback = function()
            local args = {
    [1] = workspace.Ship.Build.Switcher.hit
}

game:GetService("ReplicatedStorage").Remotes.LightSwitch:InvokeServer(unpack(args))
        end
    })
Y.Main:AddButton({
        Title = "飞船 灯光循环",
        Description = "飞船灯光循环开关",
        Callback = function()
            Fluent:Notify({
        Title = "系统",
        Content = "循环次数",
        SubContent = "正在循环30次",
        Duration = 3
        })
        for i = 1, 30 do
    local args = {
    [1] = workspace.Ship.Build.Switcher.hit
}

game:GetService("ReplicatedStorage").Remotes.LightSwitch:InvokeServer(unpack(args))
end
        end
    })
    
Y.Main:AddButton({
        Title = "飞船 显示屏控制器",
        Description = "检查人物运动的显示器",
        Callback = function()
            game:GetService("ReplicatedStorage").Remotes.ChangePlayer:FireServer()
        end
    })
